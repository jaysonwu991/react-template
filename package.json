{
  "name": "react-template",
  "version": "3.0.0",
  "private": true,
  "description": "A React Template Project",
  "author": "Jayson Wu <jaysonwu991@outlook.com>",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "test": "jest --watchAll",
    "setup": "pnpm install && husky install",
    "lint": "eslint ./ --ext .js,.jsx",
    "lint:fix": "eslint ./ --fix --ext .js,.jsx",
    "prettify": "prettier --write src/**/*.jsx",
    "build": "webpack --config config/webpack.prod.js",
    "start": "webpack serve --config config/webpack.dev.js",
    "docker:dev": "docker build -f Dockerfile.dev -t dev:builder . && docker run -v ${PWD}:/app -v /app/node_modules -p 8080:8080 --rm dev:builder",
    "docker:deploy": "docker build -f Dockerfile.prod -t prod:builder . && docker run -it -p 80:80 --rm prod:builder"
  },
  "keywords": [
    "swc",
    "jest",
    "react",
    "eslint",
    "postcss",
    "webpack",
    "prettier",
    "@swc/jest",
    "react-dom",
    "sass-embedded",
    "autoprefixer",
    "react-template",
    "@test-library/react"
  ],
  "dependencies": {
    "core-js": "^3.36.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@swc/core": "^1.4.13",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.19",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^7.1.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "esbuild-loader": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "react-refresh": "^0.14.0",
    "sass-embedded": "^1.74.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2",
    "webpack-merge": "^5.10.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "Firefox >= 20",
    "iOS >= 8",
    "Android > 4.4"
  ]
}
