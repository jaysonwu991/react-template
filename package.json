{
  "name": "react-template",
  "version": "2.0.0",
  "private": true,
  "description": "A React Template Project",
  "author": "Jayson Wu <jaysonwu991@outlook.com>",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "test": "jest --watchAll",
    "setup": "pnpm install && husky install",
    "lint": "eslint ./ --ext .js,.jsx",
    "lint:fix": "eslint ./ --fix --ext .js,.jsx",
    "prettify": "prettier --write src/**/*.jsx",
    "build": "webpack --config config/webpack.prod.js",
    "start": "webpack serve --config config/webpack.dev.js",
    "docker:dev": "docker build -f Dockerfile.dev -t dev:builder . && docker run -v ${PWD}:/app -v /app/node_modules -p 8080:8080 --rm dev:builder",
    "docker:deploy": "docker build -f Dockerfile.prod -t prod:builder . && docker run -it -p 80:80 --rm prod:builder"
  },
  "keywords": [
    "swc",
    "jest",
    "react",
    "eslint",
    "postcss",
    "webpack",
    "prettier",
    "@swc/jest",
    "react-dom",
    "sass-embedded",
    "autoprefixer",
    "react-template",
    "@test-library/react"
  ],
  "dependencies": {
    "core-js": "^3.25.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@swc/core": "^1.3.5",
    "@swc/jest": "^0.2.23",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.12",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.1",
    "esbuild-loader": "^2.20.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.17",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "sass-embedded": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "Firefox >= 20",
    "iOS >= 8",
    "Android > 4.4"
  ]
}
