{
  "name": "react-template",
  "version": "2.0.0",
  "private": true,
  "description": "A React Template Project",
  "author": "Jayson Wu <jaysonwu991@outlook.com>",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "test": "cross-env NODE_ENV=development jest --watchAll",
    "setup": "pnpm install && husky install",
    "lint": "eslint ./ --ext .js,.jsx",
    "lint:fix": "eslint ./ --fix --ext .js,.jsx",
    "prettify": "prettier --write src/**/*.{js,jsx}",
    "build": "webpack --config config/webpack.prod.js",
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "docker:dev": "docker build -f Dockerfile.dev -t dev:builder . && docker run -v ${PWD}:/app -v /app/node_modules -p 8080:8080 --rm dev:builder",
    "docker:deploy": "docker build -f Dockerfile.prod -t prod:builder . && docker run -it -p 80:80 --rm prod:builder"
  },
  "keywords": [
    "jest",
    "sass",
    "test",
    "babel",
    "react",
    "enzyme",
    "docker",
    "eslint",
    "postcss",
    "webpack",
    "prettier",
    "template",
    "unit-test",
    "autoprefixer",
    "react-template"
  ],
  "dependencies": {
    "core-js": "^3.22.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.6",
    "@swc/core": "^1.2.183",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "autoprefixer": "^10.4.7",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^9.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "esbuild-loader": "^2.18.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "react-refresh": "^0.13.0",
    "react-test-renderer": "^18.1.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "Firefox >= 20",
    "iOS >= 8",
    "Android > 4.4"
  ],
  "jest": {
    "testRegex": "((\\.|/*.)(spec))\\.js?$",
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
